export PYENV_ROOT="$PYENV_DIR"
export PATH="$PATH:$PYENV_BIN_ROOT"
eval "$(pyenv init -)"
eval "$(pyenv virtualenv-init -)"
alias workon="pyenv activate"
alias lsvirtualenv="pyenv virtualenvs"
alias mkvirtualenv="pyenv virtualenv $PYTHON_VERSION"
alias python="$ENV_PYTHON_BIN"
echo "DEACTIVATE VIRTUALENV COMMAND: . deactivate"
echo "ACTIVATE VIRTUALENV COMMAND: workon $APP_NAME"
